<!DOCTYPE HTML>
<html>
<head>
    <title>Delete Users</title>
    {{#each scripts}}
        <script type="text/javascript" src="{{this.src}}" {{this.attribute}}></script>
    {{/each}}
</head>
<body>
<div id="user-delete-header">
    <h1>Delete Users</h1>
</div>
<div id="user-delete">
    <form method="post">
        {{#each userList}}
            <div class="delete-row">
                <input type="checkbox" name="{{this.id}}" id="{{this.id}}" {{lookup ../deleteList this.id}}>
                <label for="{{this.id}}">{{this.displayName}} [{{this.userPrincipalName}}]</label>
            </div>
        {{/each}}

        <input type="submit" value="Delete">
    </form>
</div>
</body>
</html>